{
  "name": "redux-reset",
  "version": "0.1.2",
  "description": "reset redux state",
  "main": "lib/index.js",
  "scripts": {
    "lint": "./node_modules/.bin/standard  --verbose | snazzy",
    "test": "npm run lint && ./node_modules/.bin/tape -r babel-register ./test/index.test.js | tap-spec",
    "build": "./node_modules/.bin/babel src -d lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wwayne/redux-reset"
  },
  "keywords": [
    "redux",
    "react",
    "redux enhancer",
    "redux store enhancer",
    "js",
    "javascript",
    "redux component",
    "reset state"
  ],
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "lib/"
    ]
  },
  "bugs": {
    "url": "https://github.com/wwayne/redux-reset/issues"
  },
  "homepage": "https://github.com/wwayne/redux-reset",
  "author": "wwayne",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "es6-enum": "^1.0.0",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "snazzy": "^3.0.0",
    "standard": "^6.0.8",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  }
